{
    "name": "spojenet/flexibee",
    "description": "Library for easy interaction with czech accounting system FlexiBee.",
    "license": "MIT",
    "authors": [
        {
            "name": "Vítězslav Dvořák",
            "email": "vitex@arachne.cz"
        }
    ],
    "require": {
        "ext-curl": "*",
        "vitexsoftware/ease-core": ">=0.37"
    },
    "autoload": {
        "psr-4": {
            "FlexiPeeHP\\": "src/FlexiPeeHP/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\Ease\\": "vendor/vitexsoftware/ease-framework/tests/src/Ease",
            "Test\\FlexiPeeHP\\": "testing/src/FlexiPeeHP/",
            "Test\\": "testing/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "*",
	"phing/phing": "*"
    },
    "replace": {
	"spoje.net/flexipeehp": ">1.32"
    }
}
