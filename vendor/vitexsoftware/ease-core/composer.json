{
    "name": "vitexsoftware/ease-core",
    "description": "An PHP Framework for writing Applications",
    "authors": [
        {
            "name": "Vítězslav Dvořák",
            "email": "info@vitexsoftware.cz",
            "homepage": "http://v.s.cz/"
        }
    ],
    "autoload": {
        "psr-4": {
            "Ease\\": "src/Ease/",
            "Ease\\Logger\\": "src/Ease/Logger/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\Ease\\Local\\": "tests",
            "Test\\Ease\\": "tests/src/Ease",
            "Test\\Ease\\Logger\\": "tests/src/Ease/Logger",
            "Example\\Ease\\": "Examples",
            "Example\\Ease\\Logger\\": "Examples/Logger"
        }
    },
    "conflict": {
        "vitexsoftware/ease-framework": "*"
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=7",
        "pear/mail": "~1.3",
        "pear/mail_mime": "~1.10"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "php-coveralls/php-coveralls": "*",
        "jakub-onderka/php-parallel-lint": "*",
        "jakub-onderka/php-console-highlighter": "*"
    },
    "type": "library",
    "homepage": "http://v.s.cz/ease.php",
    "license": "GPL-2.0-or-later"
}
