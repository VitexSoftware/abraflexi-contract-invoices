{
    "name": "spojenet/abraflexi-contract-invoices",
    "description": "Trigger AbraFlexi's cotract to invoice generator",
    "type": "project",
    "require": {
        "spojenet/flexibee": "^3.6",
        "vitexsoftware/abraflexi-bricks": "^1.4"
    },
    "license": "MIT",
    "require-dev": {
        "phpunit/phpunit": "*",
        "phpstan/phpstan": "*",
        "friendsofphp/php-cs-fixer": "^3.61",
        "ergebnis/composer-normalize": "^2.43",
        "ergebnis/php-cs-fixer-config": "^6.34"
    },
    "authors": [
        {
            "name": "Vítězslav Dvořák",
            "email": "info@vitexsoftware.cz"
        }
    ],
    "autoload": {
        "psr-4": {
            "\\AbraFlexi\\Contracts\\": "src/AbraFlexi/Contracts/"
        }
    },
    "minimum-stability": "dev",
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        }
    }
}
